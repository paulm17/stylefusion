{
  "name": "@acme/badge",
  "private": true,
  "version": "0.0.1",
  "main": "index.js",
  "types": "./build/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.mjs",
      "require": "./build/index.js"
    }
  },
  "files": [
    "build/**"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --clean",
    "lint": "eslint \"**/*.ts*\"",
    "clean": "rimraf build .turbo",
    "typecheck": "tsc --noEmit",
    "build:fast": "tsup src",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "clean-package": "../../../../clean-package.config.json",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@stylefusion/react": "workspace:*",
    "tslib": "^2.6.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "@types/node": "^20.3.3",
    "clean-package": "2.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.1.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
